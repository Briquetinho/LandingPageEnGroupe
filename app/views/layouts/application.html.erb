<!DOCTYPE html>
<html>

<%= render 'layouts/head' %>
<body>
	<%= render 'layouts/header' %>
	<%= yield %>
	<div class="container">
		<% flash.each do |message_type, message| %>
		<div class="alert alert-<%= message_type %>">
			<%= message %>
		</div>
		<% end %>
		<%= debug(params) if Rails.env.development? %>
	</div>
</body>

</html>
